    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with --clipboard, --timestamp",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                // "${command:pickArgs}",
                "--clipboard",
                "--timestamp"
            ],
            "python": "${workspaceFolder}/venv/Scripts/python.exe"
        },
        {
            "name": "Python Debugger: Current File with --clipboard",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                // "${command:pickArgs}",
                "--clipboard",
            ],
            "python": "${workspaceFolder}/venv/Scripts/python.exe"
        },
        {
            "name": "Python Debugger: Current File with --clipboard --model base",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                // "${command:pickArgs}",
                "--clipboard",
                "--model", "base"
            ],
            "python": "${workspaceFolder}/venv/Scripts/python.exe"
        },
        {
            "name": "Python Debugger: Current File with --clipboard --model medium",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                // "${command:pickArgs}",
                "--clipboard",
                "--model", "medium"
            ],
            "python": "${workspaceFolder}/venv/Scripts/python.exe"
        }
    ]
}